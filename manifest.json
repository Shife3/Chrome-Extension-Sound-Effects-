{
  "manifest_version": 3,
  "name": "Key Sound Extension",
  "version": "1.0",
  "description": "Plays a sound when a specific key is pressed.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
